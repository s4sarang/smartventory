Handler = () => {\r\n    history.push(`/login?redirect=shipping`);\r\n  };\r\n\r\n  useEffect(() => {\r\n    if (assetLink) {\r\n      dispatch(raiseRequests(assetLink, qty));\r\n    }\r\n  }, [dispatch, assetLink, qty]);\r\n\r\n  return (\r\n    <>\r\n      <Row>\r\n        <Col md={8}>\r\n          <h1>Requests Bucket</h1>\r\n          {requestsItems.length === 0 ? (\r\n            <Message variant='warning'>\r\n              Requests Bucket is empty! <Link to='/'>Go Back</Link>\r\n            </Message>\r\n          ) : (\r\n            <ListGroup variant='flush'>\r\n              {requestsItems.map((item) => (\r\n                <ListGroup.Item key={item.link}>\r\n                  <Row>\r\n                    <Col md={2}>\r\n                      <Image\r\n                        src={item.image}\r\n                        alt={item.name}\r\n                        fluid\r\n                        rounded\r\n                      ></Image>\r\n                    </Col>\r\n                    <Col md={3}>\r\n                      <Link to={`/assets/${item.link}`}>{item.name}</Link>\r\n                    </Col>\r\n                    <Col md={2}>₹{item.price}</Col>\r\n                    <Col md={2}>\r\n                      <FormGroup>\r\n                        <FormControl\r\n                          size='sm'\r\n                          as='select'\r\n                          value={item.qty}\r\n                          onChange={(e) =>\r\n                            dispatch(\r\n                              raiseRequests(item.link, Number(e.target.value))\r\n                            )\r\n                          }\r\n                        >\r\n                          {[...Array(item.count).keys()].map((x) => (\r\n                            <option key={x + 1} value={x + 1}>\r\n                              {x + 1}\r\n                            </option>\r\n                          ))}\r\n                        </FormControl>\r\n                      </FormGroup>\r\n                    </Col>\r\n                    <Col>\r\n                      <Button\r\n                        type='button'\r\n                        variant='light'\r\n                        onClick={() => removeRequestsHandler(item.link)}\r\n                      >\r\n                        <i className='zmdi zmdi-delete'></i>\r\n                      </Button>\r\n                    </Col>\r\n                  </Row>\r\n                </ListGroup.Item>\r\n              ))}\r\n            </ListGroup>\r\n          )}\r\n        </Col>\r\n        <Col md={3}>\r\n          <ListGroup variant='flush'>\r\n            <ListGroup.Item>\r\n              <h2>\r\n                Subtotal (\r\n                {requestsItems.reduce((acc, item) => acc + item.qty, 0)}) items.\r\n              </h2>\r\n              <h5>\r\n                <Badge variant='light'>\r\n                  Total Amount: ₹\r\n                  {requestsItems.reduce(\r\n                    (acc, item) => acc + item.qty * item.price,\r\n                    0\r\n                  )}\r\n                </Badge>\r\n              </h5>\r\n            </ListGroup.Item>\r\n            <ListGroup.Item>\r\n              <Button\r\n                type='button'\r\n                className='btn-block'\r\n                variant='outline-primary'\r\n                onClick={sendHandler}\r\n                disabled={requestsItems === 0}\r\n              >\r\n                Send Request\r\n              </Button>\r\n            </ListGroup.Item>\r\n          </ListGroup>\r\n        </Col>\r\n      </Row>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default RequestsScreen;\r\n","E:\\git\\s4sarang\\smartventory\\frontend\\src\\reducers\\requestsReducers.js",[],"E:\\git\\s4sarang\\smartventory\\frontend\\src\\constants\\requestsConstants.js",[],"E:\\git\\s4sarang\\smartventory\\frontend\\src\\actions\\requestsActions.js",[],{"ruleId":"119","replacedBy":"120"},{"ruleId":"121","replacedBy"